@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}



<html>

<head>
    <title>Chelsea's eCard</title>
    <h1>Send an eGreeting to Your Friends!</h1>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>

<body>

    <div id="myCarousel" class="carousel slide col-xs-6" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/images/card_birthday.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_anniversary.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_babyshower.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_valentines.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_frame_black.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_frame_gold.png" class="img-responsiv center-block" />
            </div>

            <div class="item">
                <img src="~/images/card_frame_wood.png" class="img-responsiv center-block" />
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>


    <div class="container-fluid col-xs-6">

        <div class="row1">
            <div class="col-xs-3">
                <div class="photo img-rounded">
                    <img src="~/images/card_birthday.png" class="img-responsive img-thumbnail" />
                </div>
            </div>


            <div class="col-xs-3">
                <div class="photo img-rounded">
                    <img src="~/images/card_anniversary.png" class="img-responsive img-thumbnail" />
                </div>
            </div>


            <div class="col-xs-3">
                <div class="photo img-rounded">
                    <img src="~/images/card_babyshower.png" class="img-responsive img-thumbnail" />
                </div>
            </div>


            <div class="col-xs-3">
                <div class="photo img-rounded">
                    <img src="~/images/card_valentines.png" class="img-responsive img-thumbnail" />
                </div>
            </div>

        </div>

        <div class="row2">
            <div class="col-xs-4">
                <div class="photo img-rounded">
                    <img src="~/images/card_frame_black.png" class="img-responsive img-thumbnail" />
                </div>
            </div>

            <div class="col-xs-4">
                <div class="photo img-rounded">
                    <img src="~/images/card_frame_gold.png" class="img-responsive img-thumbnail" />
                </div>
            </div>

            <div class="col-xs-4">
                <div class="photo img-rounded">
                    <img src="~/images/card_frame_wood.png" class="img-responsive img-thumbnail" />
                </div>
            </div>

        </div>
    </div>



